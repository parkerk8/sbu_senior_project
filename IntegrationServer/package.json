		"db:migrate": "sequelize db:migrate",
		"stop": "kill-port 3000",
		"test": "mocha",
		"testWithCoverage": "nyc --reporter=html mocha && python3 main.py > ./src/coverage/index.html",
		"debug": "node --inspect src/server.js"
	},
	"nodemonConfig": {
		"watch": [
			".env",
			"src/"
